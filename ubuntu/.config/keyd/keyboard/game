[main]
capslock = overload(l1, esc)


[l1]
1 = f1
2 = f2
3 = f3
4 = f4
5 = f5
6 = f6

w = up
a = left
s = down
d = right

h = left
j = down
k = up
l = right
; = home
' = end

o = backspace
p = A-M-p
u = A-M-u 
/ = A-M-/

tab = capslock
[ = print
f = volumeup
v = volumedown
c = mute
g = A-M-g
b = A-M-b


[shift+l1]
1 = kp1
2 = kp2
3 = kp3
4 = kp4
5 = kp5
6 = kp6
7 = kp7
8 = kp8
9 = kp9
0 = kp0
- = kpminus
= = kpequal
