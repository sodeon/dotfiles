#---------------------------------------------------------------------------------------------------
# Next/prev tab
"xte 'keydown Control_L' 'key Prior' 'keyup Control_L'"
b:6
"xte 'keydown Control_L' 'key Next' 'keyup Control_L'"
b:7

# Move tab
"xte 'keyup XF86Launch5' 'keydown Control_L' 'keydown Shift_L' 'key Prior' 'keyup Shift_L' 'keyup Control_L' 'keydown XF86Launch5'"
Mod5+b:6
"xte 'keyup XF86Launch5' 'keydown Control_L' 'keydown Shift_L' 'key Next'  'keyup Shift_L' 'keyup Control_L' 'keydown XF86Launch5'"
Mod5+b:7
