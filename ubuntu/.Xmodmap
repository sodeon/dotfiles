!------------------------------------------------------------------------------
! Customized layout
!------------------------------------------------------------------------------
! Typing assist
keycode 29 = y Y Delete    Delete
keycode 32 = o O BackSpace BackSpace
! keycode 31 = i I Delete    Delete

! Right hand navigation (HJKL)
keycode 43 = h H Left  Left
keycode 44 = j J Down  Down
keycode 45 = k K Up    Up
keycode 46 = l L Right Right

keycode 58 = m          M        Next  Next
keycode 59 = comma      less     Prior Prior
keycode 47 = semicolon  colon    Home  Home
keycode 48 = apostrophe quotedbl End   End

! Left hand navigation (WASD)
keycode 25 = w W Up    Up
keycode 38 = a A Left  Left
keycode 39 = s S Down  Down
keycode 40 = d D Right Right
! keycode 24 = q Q Prior Prior
! keycode 26 = e E Next  Next

keycode 52 = z Z j J
keycode 53 = x X l L

! Function keys
keycode 10 = 1     exclam      F1
keycode 11 = 2     at          F2
keycode 12 = 3     numbersign  F3
keycode 13 = 4     dollar      F4
keycode 14 = 5     percent     F5
keycode 15 = 6     asciicircum F6
keycode 16 = 7     ampersand   F7
keycode 17 = 8     asterisk    F8
keycode 18 = 9     parenleft   F9
keycode 19 = 0     parenright  F10
keycode 20 = minus underscore  F11
keycode 21 = equal plus        F12

